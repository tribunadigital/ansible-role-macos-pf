<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>{{ macos_pf_anchor }}.pfctl</string>
    <key>Program</key>
    <string>{{ macos_pf_script_path }}</string>
    <key>RunAtLoad</key>
    <true/>
    <key>LaunchOnlyOnce</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/{{ ansible_user }}/pfctl_log.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/{{ ansible_user }}/pfctl_error.log</string>
</dict>
</plist>
